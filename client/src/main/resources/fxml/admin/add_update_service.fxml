<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" styleClass="adminbg"
            stylesheets="@../../css/background.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="com.example.bsuir.controller.admin.AddUpdateServiceController">
    <children>
        <TextField fx:id="nameField" layoutX="220.0" layoutY="109.0"/>
        <TextField fx:id="countryField" layoutX="220.0" layoutY="150.0"/>
        <TextField fx:id="priceField" layoutX="219.0" layoutY="191.0"/>
        <Button fx:id="btnLogOut" layoutX="451.0" layoutY="2.0" mnemonicParsing="false" onAction="#onLogOut"
                styleClass="glass" stylesheets="@../../css/stylesheet.css" text="Выйти из аккаунта"/>
        <Button fx:id="btnBack" layoutX="391.0" layoutY="2.0" mnemonicParsing="false" onAction="#onBack"
                styleClass="glass" stylesheets="@../../css/stylesheet.css" text="Назад"/>
        <Label layoutX="108.0" layoutY="155.0" text="Страна: ">
            <font>
                <Font name="System Bold Italic" size="13.0"/>
            </font>
        </Label>
        <Label layoutX="170.0" layoutY="196.0" text="Цена: ">
            <font>
                <Font name="System Bold Italic" size="13.0"/>
            </font>
        </Label>
        <Button fx:id="btnSave" layoutX="260.0" layoutY="243.0" mnemonicParsing="false" onAction="#onSave"
                styleClass="glass" stylesheets="@../../css/stylesheet.css" text="Сохранить"/>
        <Label layoutX="159.0" layoutY="114.0" text="Рейс: ">
            <font>
                <Font name="System Bold Italic" size="13.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
