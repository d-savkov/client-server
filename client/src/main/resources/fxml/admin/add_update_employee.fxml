<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" styleClass="adminbg"
            stylesheets="@../../css/background.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="com.example.bsuir.controller.admin.AddUpdateEmployeeController">
    <children>
        <TextField fx:id="nameField" layoutX="220.0" layoutY="134.0"/>
        <TextField fx:id="surnameField" layoutX="220.0" layoutY="175.0"/>
        <Button fx:id="btnLogOut" layoutX="451.0" layoutY="2.0" mnemonicParsing="false" onAction="#onLogOut"
                styleClass="glass" stylesheets="@../../css/stylesheet.css" text="Выйти из аккаунта"/>
        <Button fx:id="btnBack" layoutX="391.0" layoutY="2.0" mnemonicParsing="false" onAction="#onBack"
                styleClass="glass" stylesheets="@../../css/stylesheet.css" text="Назад"/>
        <Label layoutX="172.0" layoutY="139.0" prefHeight="16.0" prefWidth="42.0" text="Имя: ">
            <font>
                <Font name="System Bold Italic" size="13.0"/>
            </font>
        </Label>
        <Label layoutX="142.0" layoutY="180.0" text="Фамилия: ">
            <font>
                <Font name="System Bold Italic" size="13.0"/>
            </font>
        </Label>
        <Button fx:id="btnSave" layoutX="254.0" layoutY="227.0" mnemonicParsing="false" onAction="#onSave"
                styleClass="glass" stylesheets="@../../css/stylesheet.css" text="Сохранить"/>
    </children>
</AnchorPane>
