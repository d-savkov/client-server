<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" styleClass="adminbg"
            stylesheets="@../../css/background.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="com.example.bsuir.controller.admin.StatisticController">
    <children>
        <BarChart fx:id="barChart" layoutX="52.0" layoutY="10.0" prefHeight="381.0" prefWidth="472.0"
                  title="Актуальность аэропорта">
            <xAxis>
                <CategoryAxis fx:id="axisX" label="Дата" side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="axisY" label="Количество рейсов" side="LEFT"/>
            </yAxis>
        </BarChart>
        <Button fx:id="btnBack" layoutX="524.0" layoutY="361.0" mnemonicParsing="false" onAction="#onBack"
                styleClass="glass" stylesheets="@../../css/stylesheet.css" text="Назад"/>
    </children>
</AnchorPane>
